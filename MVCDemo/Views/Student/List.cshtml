
@{
    ViewData["Title"] = "List";
}

<h1>List of Students </h1>

<table class="table">
    <tr>
        <th> ID </th>
        <th> Name </th>
        <th> Address </th>
        <th> Score </th>
    </tr>

 @{foreach(var student in ViewBag.list)
 {
            <tr>
                <td> @student.Id </td>
                <td> @student.Name </td>
                <td> @student.Address </td>
                <td> @student.Score</td>
             </tr>
 }
 }
</table>
